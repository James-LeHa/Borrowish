{
  "name": "borrowish",
  "version": "1.0.0",
  "scripts": {
    "install-client": "cd client && npm install",
    "client-test": "cd client && npm test",
    "client": "cd client && npm start",
    "server": "nodemon server/server.ts",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "@types/express": "^4.17.4",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
